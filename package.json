{
  "name": "@form8ion/eslint-config",
  "description": "shareable configuration for ESLint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=12.20"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/eslint-config",
  "bugs": "https://github.com/form8ion/eslint-config/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "generate:md": "remark . --output",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.25",
    "@form8ion/remark-lint-preset": "2.1.7",
    "ban-sensitive-files": "1.9.16",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "ls-engines": "0.6.1",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.1",
    "remark-toc": "8.0.1"
  },
  "dependencies": {
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.25.3"
  }
}
