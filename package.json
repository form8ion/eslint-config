{
  "name": "@form8ion/eslint-config",
  "description": "shareable configuration for ESLint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/eslint-config",
  "bugs": "https://github.com/form8ion/eslint-config/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "generate:md": "remark . --output"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.18",
    "@form8ion/remark-lint-preset": "1.0.13",
    "ban-sensitive-files": "1.9.15",
    "cz-conventional-changelog": "3.3.0",
    "husky": "6.0.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0",
    "remark-toc": "7.2.0"
  },
  "dependencies": {
    "eslint": "7.26.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.22.1"
  }
}
